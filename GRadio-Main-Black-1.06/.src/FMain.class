Private Sub cargaContenido(ruta As String, l As ListBox)

  Dim linea As New String[]
  Dim a As Integer
  Dim contenido As String

  
  Try linea = Split(File.Load(ruta), "\n")
 'If Error Then Return
  l.Clear()
  For a = 0 To linea.count - 1
